{{#if entryData.length}}
{{#each entryData}}
<div class='timeentryCont'>
	<div class ='desc'><p>{{this.notes}}</p></div>
	<div class ='hourcont'>
		<div class='hour'>{{this.hour}}</div>
	</div>
	<div class='icones'>
		<span class='edit'>&nbsp;</span>
		<span class='remove'>&nbsp;</span>
	</div>
	<div class='clear'/>
</div>
 {{/each}}
 {{/if}}
<div class='clear'/>